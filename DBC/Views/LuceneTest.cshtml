@inherits UmbracoTemplatePage
@using DBC.Controllers

@{
	Layout = "Master.cshtml";
    var umbracoHelperSearchResults = new TestController().UmbracoContentService(Model.Content);
    var umbracoCSSearchResults = new TestController().UmbracoHelper();
    var lucenSearchResults = new TestController().Lucene();
    var mongoSearchResults = new TestController().Mongo();
    var petaPocoSearchResults = new TestController().PetaPoco();
    var examineSearchResults = new TestController().Examine();
}

@Html.Partial("~/Views/Partials/SectionHeader.cshtml")

<style>
    table {
        width: 600px;
    }

    th {
        text-align: left
    }
</style>

<section class="section">
    <div class="container">
        @Html.Raw(umbracoCSSearchResults)
        <br />
        @Html.Raw(umbracoHelperSearchResults)
        <br />
        @Html.Raw(lucenSearchResults)
        <br />
        @Html.Raw(mongoSearchResults)
        <br />
        @Html.Raw(petaPocoSearchResults)
        <br />
        @Html.Raw(examineSearchResults)
    </div>
</section>




